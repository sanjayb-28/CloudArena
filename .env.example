ENV=dev
REGION=us-east-1
ARENA_ACCOUNT_ID=<your-aws-account-id>
AUTH0_DOMAIN=<your-tenant>.us.auth0.com
AUTH0_AUDIENCE=https://cloudarena.api
AUTH0_ISSUER=https://<your-tenant>.us.auth0.com/
AUTH0_JWKS_URI=https://<your-tenant>.us.auth0.com/.well-known/jwks.json
AUTH0_CLIENT_ID=<auth0-client-id>
AUTH0_CLIENT_SECRET=<auth0-client-secret>
AUTH0_CALLBACK_URL=http://localhost:8000/auth/callback
AUTH0_LOGOUT_REDIRECT_URL=http://localhost:8000/
GEMINI_API_KEY=<gemini-api-key>
AWS_PROFILE=arena
USE_GRADIENT=0
DATABASE_URL=sqlite:////data/cloudarena.db
AUTH_TOKEN=<worker-shared-secret>
SESSION_SECRET=<generate-a-strong-random-value>
SESSION_COOKIE_SECURE=0
